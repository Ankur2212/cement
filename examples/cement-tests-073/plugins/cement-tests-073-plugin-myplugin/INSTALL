
SOURCE INSTALLATION:

    $ sudo python setup.py install

    # The following is one command
    $ sudo cp -a ./etc/cement-tests-073/plugins.d/myplugin.conf \
                 /etc/cement-tests-073/plugins.d/
    
    
USAGE:

    $ cement-tests-073 --help
    
    $ cement-tests-073 myplugin --help